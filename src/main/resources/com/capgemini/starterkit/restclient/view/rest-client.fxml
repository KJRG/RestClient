<AnchorPane prefHeight="455.0" prefWidth="560.0" xmlns="http://javafx.com/javafx/8.0.51" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.capgemini.starterkit.restclient.controller.RestClientController">

  <Label layoutX="12.0" layoutY="12.0" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefHeight="17.0" prefWidth="538.0" styleClass="formTitle" text="%label.formTitle" />
  <TextField fx:id="titleField" layoutX="12.0" layoutY="66.0" prefWidth="250.0" promptText="%textField.title" />
  <Button fx:id="searchButton" defaultButton="true" layoutX="12.0" layoutY="96.0" mnemonicParsing="false" onAction="#searchButtonAction" prefWidth="110.0" text="%button.search" />
  <Button fx:id="deleteButton" layoutX="12.0" layoutY="126.0" mnemonicParsing="false" onAction="#deleteButtonAction" prefWidth="110.0" text="%button.delete" />
