    <columns>
      <TableColumn fx:id="idColumn" minWidth="36.0" prefWidth="36.0" text="%column.id" />
      <TableColumn fx:id="titleColumn" minWidth="250.0" prefWidth="250.0" text="%column.title" />
      <TableColumn fx:id="authorsColumn" minWidth="250.0" prefWidth="250.0" text="%column.authors" />
    </columns>
  </TableView>
  <TextField fx:id="bookTitleField" layoutX="298.0" layoutY="66.0" prefWidth="250.0" promptText="%textField.addBookTitle" />
  <TextField fx:id="authorsField" layoutX="298.0" layoutY="96.0" prefWidth="250.0" promptText="%textField.addBookAuthors" />
